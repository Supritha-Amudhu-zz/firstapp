<% microposts.each do |micropost| %>
	<tr>
		<td class = "micropost">
			<br/><span class = "content"><%= h micropost.content %></span><br/>
			<span class = "timestamp">
				Posted <%= time_ago_in_words(micropost.created_at) %> ago.
		<td id="span2">
			<%= link_to "delete", micropost, :method => :delete, :confirm => "You Sure?" if current_user?(micropost.user) %>
		</td>
			</span>
			
		</td>
		
		</td>
	</tr>
<% end %>


